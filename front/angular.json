{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "projects": {
    "proy-plus": {
      "projectType": "application",
      "root": "",
      "sourceRoot": "src",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/proy-plus",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              { "glob": "**/*", "input": "public", "output": "/" }
            ],
            "styles": [
              "primeflex/primeflex.css",
              "primeicons/primeicons.css",
              "src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "extractLicenses": true,
              "sourceMap": false
            },
            "development": {
              "optimization": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "development"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": { "buildTarget": "proy-plus:build" },
          "configurations": {
            "local": {
              "buildTarget": "proy-plus:build:development",
              "proxyConfig": "proxy.local.js"
            },
            "docker": {
              "buildTarget": "proy-plus:build:development",
              "proxyConfig": "proxy.docker.js",
              "host": "0.0.0.0",
              "port": 4200,
              "allowedHosts": ["www.proyplus.com", "72.60.11.35"],
              "publicHost": "www.proyplus.com",
              "liveReload": false
            }
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "add0a733-410d-49fc-96bf-c073db1f43c8"
  }
}
